alert();
$(document).ready(
		function() {
			alert();
			var content = "";
			for (var i = 1; i < 9; i++) {
				for (var j = 1; j < 9; j++) {
					if (i % 2 === j % 2) {
						content += "<div id='" + i + "." + j
								+ "' class='white'></div>";
					} else {
						content += "<div id='" + i + "." + j
								+ "' class='black'></div>";
					}
				}
				content += "<br><br><br>";
			}
			$(".div").append(content);
		});