$(document).ready(function() {
	var content = "";
	for(var i = 0; i < 8; i++){
		content += "<tr>";
		for(var j = 1; j < 9; j++){
			if(j % 2 === i % 2){
					content += "<td class='black'></td>"
				}
			}else{
				content += "<td class='white'></td>"
			}
		}
		content += "</tr>";
		content += "/n";
	}
	$(".tbody").append(content);
});