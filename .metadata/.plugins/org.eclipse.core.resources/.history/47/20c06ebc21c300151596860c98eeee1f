var i;
var j;
$(document)
		.ready(
				function() {
					for (i = 1; i < 9; i++) {
						$(".board").append('<div class="row"></div>');
						for (j = 1; j < 9; j++) {
							if (i === 2) {
								$("div.row:last")
										.append(
												'<div class="cell"><img src="file:///home/heresy/Documents/KARTHEEK/chess%20icons/Pawn.jpg"></div>');
							} else if(i === 7){
								$("div.row:last")
								.append(
										'<div class="cell"><img src="file:///home/heresy/Documents/KARTHEEK/chess%20icons/Pawn.jpg"></div>');
							}else {
								$("div.row:last").append('<div class="cell">');
							}
						}
					}
				});